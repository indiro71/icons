{
  "private": true,
  "name": "@indiro/icons",
  "version": "1.0.53",
  "scripts": {
    "clean": "rm -rf src/icons && rm -rf lib",
    "svgr": "svgr --icon --title-prop --no-dimensions --typescript -d src/icons assets",
    "build": "npm run clean && npm run svgr && rollup -c",
    "release": "npm run build && git stash && npm version patch && git stash pop && NODE_ENV=production node scripts/release.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "main": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@svgr/cli": "^5.5.0",
    "@svgr/rollup": "^5.5.0",
    "@types/react": "^17.0.19",
    "babel-loader": "^8.2.2",
    "babel-plugin-import-glob": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "dependencies": {
    "styled-components": "^5.3.6"
  }
}
